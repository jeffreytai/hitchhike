          url='/trips/new'
          version='1.1'
          method='POST'
          contents='utf8=%E2%9C%93&amp;trip%5Btitle%5D=Test&amp;trip%5Bdescription%5D=testing&amp;trip%5Blocations%5D=%5B%7B%22lat%22%3A33.97548099999999%2C%22lng%22%3A-118.4632426%2C%22name%22%3A%22Random+Action%22%7D%2C%7B%22lat%22%3A34.050927%2C%22lng%22%3A-118.459968%2C%22name%22%3A%22Way+of+the+Future%22%7D%2C%7B%22lat%22%3A33.98971580000001%2C%22lng%22%3A-118.46321319999998%2C%22name%22%3A%22Another+Kind+of+Sunrise%22%7D%5D&amp;trip%5Btags%5D=%23test&amp;trip%5Bpostdate%281i%29%5D=2015&amp;trip%5Bpostdate%282i%29%5D=11&amp;trip%5Bpostdate%283i%29%5D=12&amp;trip%5Bpostdate%284i%29%5D=19&amp;trip%5Bpostdate%285i%29%5D=25&amp;commit=Create+Trip'>
        </http>
      </request>

      <!-- wait for up to 2 seconds, user decides to delete trip -->
      <thinktime value="2" random="true"></thinktime>

      <!-- edit the trip we just created (fill out later) -->

